<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.k2js.p1.match.MatchDAO">

<select id="matchList" parameterType="Date" resultType="MatchVO">
	select * from match where matchTime=#{matchTime}
</select>

<select id="matchSelect" parameterType="Long" resultType="MatchVO">
	select * from match where num=#{num}
</select>






</mapper>