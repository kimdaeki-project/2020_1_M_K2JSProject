<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.k2js.p1.match.MatchDAO">


<select id="matchList" parameterType="java.lang.Integer" resultType="MatchVO">
	select * from match where to_char(matchTime,'dd') = #{day}


<select id="matchSelect" parameterType="Long" resultType="MatchVO">
	select * from match where num=#{num}
</select>




</mapper>